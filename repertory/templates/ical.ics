BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//hacksw/handcal//NONSGML v1.0//EN
CALSCALE:GREGORIAN{% for event in events %}
BEGIN:VEVENT
DTEND:{{ event.endtime|date:"Ymd\THis\Z" }}
UID:austinrep-{{ event.id }}
DTSTAMP:{% now "Ymd\THis\Z" %}
LOCATION:{{ event.venue.name }}
SUMMARY:{{ event.title }}
{% comment %}URL;VALUE=URI:<?= escapeString($uri) ?>
{% endcomment %}DESCRIPTION:{% if event.series %}{{ event.series.name }}{% endif %}
DTSTART:{{ event.datetime|date:"Ymd\THis\Z" }}
END:VEVENT
{% endfor %}
END:VCALENDAR